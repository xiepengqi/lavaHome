(/-doMath
     (if (not (def? $0))
         (return 0))
     (if (not (bl $1))
         (return 0))
     (if (< (len $1) 2)
        (return (get $1 0i)))
     (def op $0)
     (def result (get $1 0i))
     (foreach (subList $1 1i (parseInt (len $1)))
         /sum)
     (/sum
         (= result (op (as result) $0)))
     result)

