(/sort:arr
    (if (not (def? arr))
        (return))
    (for (/:i
        (for (/:j
            (if (> (get arr j) (get arr (+ j 1)))
                (do
                    (def temp (get arr j))
                    (set arr j (get arr (+ j 1)))
                    (set arr (+ j 1) temp)))) 
			0 (- (len arr) (+ i 2))))
		0 (- (len arr) 2))
    arr)

(/get:obj:key
    (if (== obj null)
        (return null))
    (if (inf obj "java.util.Map")
        (return (.get obj (as "java.lang.Object" key))))
    (if (catch
            (if (inf obj "java.util.List")
                     (return (.get obj (parseInt (str key))))))
        (return null)))

(/set:obj:key:value
    (if (== obj null)
        (return null))
    (= value (as "java.lang.Object" value))
    (if (inf obj "java.util.List")
        (.set obj (parseInt key) value))
    (if (inf obj "java.util.Map")
        (.put obj (parseInt key) value))
    obj)

(/add:list:*args
    (if (or (== list null) (== args null))
        (return list))
    (.addAll list (as 'java.util.Collection' args))
    list)

(/put:map:*args
    (if (or (== map null) (== args null))
        (return map))
    (def index 0)
    (/doSub:i
        (= index (+ i 2))
        (.put map (get args i) (get args (+ i 1))))
    (for /doSub 0 2 (.size args))
    (if (not (== (get args index) null))
        (.put map (get args index) (asObj null)))
    map)

